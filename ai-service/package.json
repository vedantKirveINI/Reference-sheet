{
  "name": "ai-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npx tsx watch src/server.ts",
    "build": "npx tsc",
    "start": "node dist/server.js",
    "test": "npx jest --verbose"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^3.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "openai": "^4.77.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
}
