{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@teable/v2-benchmark-bun",
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "customConditions": ["@teable/source"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": false,
    "incremental": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationDir": "dist",
    "composite": true,
    "rootDir": "../../",
    "outDir": "dist",
    "paths": {
      "@teable/v2-container-bun-test": ["../container-bun-test/src"],
      "@teable/v2-contract-http-client": ["../contract-http-client/src"],
      "@teable/v2-contract-http-express": ["../contract-http-express/src"],
      "@teable/v2-contract-http-fastify": ["../contract-http-fastify/src"],
      "@teable/v2-contract-http-hono": ["../contract-http-hono/src"],
      "@teable/v2-contract-http-implementation": ["../contract-http-implementation/src"],
      "@teable/v2-contract-http": ["../contract-http/src"],
      "@teable/v2-contract-http-openapi": ["../contract-http-openapi/src"],
      "@teable/v2-adapter-table-repository-postgres": ["../adapter-table-repository-postgres/src"],
      "@teable/v2-formula-sql-pg": ["../formula-sql-pg/src"],
      "@teable/v2-adapter-db-postgres-shared": ["../adapter-db-postgres-shared/src"],
      "@teable/v2-adapter-repository-postgres": ["../adapter-repository-postgres/src"],
      "@teable/v2-adapter-record-repository-postgres": [
        "../adapter-record-repository-postgres/src"
      ],
      "@teable/v2-adapter-csv-parser-papaparse": ["../adapter-csv-parser-papaparse/src"],
      "@teable/v2-adapter-logger-console": ["../adapter-logger-console/src"],
      "@teable/v2-adapter-db-postgres-pglite": ["../adapter-db-postgres-pglite/src"],
      "@teable/v2-command-explain": ["../command-explain/src"],
      "@teable/v2-field-dependency-core": ["../field-dependency-core/src"],
      "@teable/v2-container-node": ["../container-node/src"],
      "@teable/v2-container-node-test": ["../container-node-test/src"],
      "@teable/v2-core": ["../core/src"],
      "@teable/v2-table-templates": ["../table-templates/src"],
      "@teable/v2-adapter-db-postgres-pg": ["../adapter-db-postgres-pg/src"],
      "@teable/v2-adapter-db-postgres-bun-sql": ["../adapter-db-postgres-bun-sql/src"],
      "@teable/v2-adapter-db-postgres-postgresjs": ["../adapter-db-postgres-postgresjs/src"],
      "@teable/v2-di": ["../di/src"],
      "@teable/v2-postgres-schema": ["../postgres-schema/src"],
      "@teable/formula": ["../../formula/src"]
    },
    "types": ["node"]
  },
  "exclude": [
    "**/node_modules",
    "**/.*/",
    "./dist",
    "./coverage",
    "../**/*.spec.ts",
    "../formula-sql-pg/src/testkit/**",
    "../../formula/src/**/*.spec.ts"
  ],
  "include": [
    "src",
    "../container-bun-test/src",
    "../contract-http-client/src",
    "../contract-http-express/src",
    "../contract-http-fastify/src",
    "../contract-http-hono/src",
    "../contract-http-openapi/src",
    "../contract-http-implementation/src",
    "../contract-http/src",
    "../adapter-table-repository-postgres/src",
    "../adapter-repository-postgres/src",
    "../adapter-record-repository-postgres/src",
    "../adapter-csv-parser-papaparse/src",
    "../adapter-logger-console/src",
    "../adapter-db-postgres-pglite/src",
    "../command-explain/src",
    "../field-dependency-core/src",
    "../container-node/src",
    "../container-node-test/src",
    "../core/src",
    "../table-templates/src",
    "../adapter-db-postgres-pg/src",
    "../adapter-db-postgres-bun-sql/src",
    "../adapter-db-postgres-postgresjs/src",
    "../adapter-db-postgres-shared/src",
    "../formula-sql-pg/src",
    "../di/src",
    "../postgres-schema/src",
    "../../formula/src"
  ]
}
