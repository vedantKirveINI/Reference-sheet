{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@teable/v2-contract-http-express",
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "esnext",
    "lib": ["esnext"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": false,
    "incremental": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationDir": "dist",
    "composite": true,
    "rootDir": "../",
    "outDir": "dist",
    "paths": {
      "@teable/v2-contract-http-openapi": ["../contract-http-openapi/src"],
      "@teable/v2-contract-http-implementation": ["../contract-http-implementation/src"],
      "@teable/v2-contract-http": ["../contract-http/src"],
      "@teable/v2-container-node": ["../container-node/src"],
      "@teable/v2-adapter-table-repository-postgres": ["../adapter-table-repository-postgres/src"],
      "@teable/v2-adapter-repository-postgres": ["../adapter-repository-postgres/src"],
      "@teable/v2-core": ["../core/src"],
      "@teable/v2-adapter-db-postgres-pg": ["../adapter-db-postgres-pg/src"],
      "@teable/v2-di": ["../di/src"]
    },
    "types": ["vitest/globals"]
  },
  "exclude": ["**/node_modules", "**/.*/", "./dist", "./coverage"],
  "include": [
    "src",
    "../contract-http-openapi/src",
    "../contract-http-implementation/src",
    "../contract-http/src",
    "../container-node/src",
    "../adapter-table-repository-postgres/src",
    "../adapter-repository-postgres/src",
    "../core/src",
    "../adapter-db-postgres-pg/src",
    "../di/src"
  ]
}
