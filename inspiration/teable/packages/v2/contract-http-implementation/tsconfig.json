{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@teable/v2-contract-http-implementation",
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "esnext",
    "lib": ["esnext"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": false,
    "incremental": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationDir": "dist",
    "composite": true,
    "rootDir": "../../",
    "outDir": "dist",
    "paths": {
      "@teable/v2-contract-http": ["../contract-http/src"],
      "@teable/v2-container-node": ["../container-node/src"],
      "@teable/v2-container-node-test": ["../container-node-test/src"],
      "@teable/v2-adapter-table-repository-postgres": ["../adapter-table-repository-postgres/src"],
      "@teable/v2-adapter-repository-postgres": ["../adapter-repository-postgres/src"],
      "@teable/v2-core": ["../core/src"],
      "@teable/v2-adapter-db-postgres-pg": ["../adapter-db-postgres-pg/src"],
      "@teable/v2-adapter-db-postgres-shared": ["../adapter-db-postgres-shared/src"],
      "@teable/v2-adapter-logger-console": ["../adapter-logger-console/src"],
      "@teable/v2-di": ["../di/src"],
      "@teable/formula": ["../../formula/src"]
    },
    "types": ["vitest/globals", "node"]
  },
  "exclude": [
    "**/node_modules",
    "**/.*/",
    "./dist",
    "./coverage",
    "../core/src/**/*.spec.ts",
    "../core/src/**/*.test.ts",
    "../core/src/**/__tests__/**",
    "../adapter-table-repository-postgres/src/**/*.spec.ts",
    "../adapter-table-repository-postgres/src/**/*.test.ts",
    "../adapter-table-repository-postgres/src/**/__tests__/**",
    "../adapter-repository-postgres/src/**/*.spec.ts",
    "../adapter-repository-postgres/src/**/*.test.ts",
    "../adapter-repository-postgres/src/**/__tests__/**",
    "../formula-sql-pg/src/**/*.spec.ts",
    "../formula-sql-pg/src/**/*.test.ts",
    "../formula-sql-pg/src/**/__tests__/**",
    "../formula-sql-pg/src/testkit/**",
    "../../formula/src/**/*.spec.ts"
  ],
  "include": [
    "src",
    "../contract-http/src",
    "../container-node/src",
    "../container-node-test/src",
    "../adapter-table-repository-postgres/src",
    "../adapter-repository-postgres/src",
    "../core/src",
    "../adapter-db-postgres-pg/src",
    "../adapter-db-postgres-shared/src",
    "../adapter-logger-console/src",
    "../di/src",
    "../../formula/src"
  ]
}
