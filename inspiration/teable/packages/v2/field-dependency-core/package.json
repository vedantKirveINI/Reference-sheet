{
  "name": "@teable/v2-field-dependency-core",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "@teable/source": "./src/index.ts",
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdown --tsconfig tsconfig.build.json",
    "dev": "tsdown --tsconfig tsconfig.build.json --watch",
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo ./tsconfig.build.tsbuildinfo ./.eslintcache",
    "lint": "eslint . --ext .ts,.js,.mjs,.cjs,.mts,.cts --cache --cache-location ../../../.cache/eslint/v2-field-dependency-core.eslintcache",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "test": "vitest run",
    "fix-all-files": "eslint . --ext .ts,.js,.mjs,.cjs,.mts,.cts --fix"
  },
  "dependencies": {
    "@teable/v2-core": "workspace:*",
    "neverthrow": "8.2.0"
  },
  "devDependencies": {
    "@teable/v2-tsdown-config": "workspace:*",
    "@teable/eslint-config-bases": "workspace:^",
    "@types/node": "22.18.0",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "tsdown": "0.18.1",
    "typescript": "5.4.3",
    "vitest": "4.0.17"
  }
}
